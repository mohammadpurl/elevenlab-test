{
    "devCommand": "uvicorn api.app:app --host 127.0.0.1 --port 4800 --reload",
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": [
                    "dist/**/*"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}